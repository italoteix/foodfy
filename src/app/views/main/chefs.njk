{% extends 'main/layout.njk' %}

{% block content %}
  <section class="wrapper admin">

    <div class="chefs">
      {% for chef in chefs %}
        <div class="chef">
          <img src={{chef.avatar_url}} alt={{chef.name}}>
          <p>{{ chef.name }}</p>
          <a href="/admin/chefs/{{ chef.id }}">Visualizar</a>
        </div>
      {% endfor %}
    </div>
  </section>
{% endblock content %}
